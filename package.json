{
  "name": "react-auto-page-router",
  "private": false,
  "version": "0.0.6",
  "type": "module",
  "main": "dist/react-auto-page-router.umd.js",
  "module": "dist/react-auto-page-router.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsc   && vite build",
    "type-check": "tsc --noEmit",
    "rebuild": "rm -rf dist && tsc   && vite build",
    "prepare": "npm run rebuild"
  },
  "dependencies": {
    "vite": "^5.2.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.23.1"
  },
  "exports": {
    ".": {
      "import": "./dist/react-auto-page-router.es.js",
      "require": "./dist/react-auto-page-router.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "typescript": "^5.2.2",
    "vite-plugin-dts": "^3.9.1"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BinovaGautam/react-auto-page-router.git"
  }
}
